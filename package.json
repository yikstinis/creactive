{
  "name": "creactive",
  "version": "0.0.53",
  "main": ".storybook",
  "files": [
    "build/**"
  ],
  "scripts": {
    "prestart": "sb-rn-get-stories && node scripts/update-package.js .storybook",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "test": "npm run test:android && npm run test:ios && npm run test:web",
    "test:android": "jest",
    "test:ios": "jest",
    "test:web": "jest",
    "lint": "expo lint",
    "build": "webpack --mode production && tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "prepublishOnly": "npm run build && node scripts/update-package.js build/index.js",
    "postpublish": "node scripts/update-package.js .storybook"
  },
  "peerDependencies": {
    "react": ">= 18 < 19",
    "react-dom": ">= 18 < 19",
    "react-native": ">= 0.68 < 0.78",
    "react-native-web": ">= 0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@faker-js/faker": "^9.2.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@storybook/addon-ondevice-actions": "^8.4.2",
    "@storybook/addon-ondevice-controls": "^8.4.2",
    "@storybook/react-native": "^8.4.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-native-web": "^0.19.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-loader": "^8.4.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "expo": "^52.0.36",
    "jest": "^29.2.1",
    "jest-expo": "^52.0.5",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.7",
    "react-native-web": "^0.19.13",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
