{
  "name": "creactive",
  "version": "0.0.23",
  "main": ".storybook",
  "files": [
    "build/**"
  ],
  "scripts": {
    "preinstall": "node scripts/update-package-dependencies.js dependencies",
    "postinstall": "node scripts/update-package-dependencies.js peerDependencies",
    "prestart": "sb-rn-get-stories && node scripts/update-package-main.js .storybook",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build": "npm run build:default && npm run build:web",
    "build:default": "webpack --mode production && tsc -p tsconfig.default.json && tsc-alias -p tsconfig.default.json",
    "build:web": "webpack --mode production && tsc -p tsconfig.web.json && tsc-alias -p tsconfig.web.json",
    "prepublishOnly": "node scripts/update-package-main.js build/default/index.js && node scripts/update-package-dependencies.js peerDependencies",
    "postpublish": "node scripts/update-package-main.js .storybook"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-web": "0.19.13"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@faker-js/faker": "^9.2.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@storybook/addon-ondevice-actions": "^8.4.2",
    "@storybook/addon-ondevice-controls": "^8.4.2",
    "@storybook/react-native": "^8.4.2",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-native-web": "^0.19.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-loader": "^8.4.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "expo": "^52.0.35",
    "jest": "^29.2.1",
    "jest-expo": "^52.0.4",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
